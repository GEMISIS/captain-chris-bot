{
    "env": {
      "browser": true,
      "es2022": true
    },
    "extends": [
      "google"
    ],
    "ignorePatterns": ["build/**", "dist/**", "node_modules/**"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "indent": ["error", 4],
      "object-curly-spacing": ["error", "always"],
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-multi-str": "off"
    }
  }
  